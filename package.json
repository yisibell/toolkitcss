{
  "name": "toolkitcss",
  "version": "2.1.1",
  "description": "A CSS toolkit class compiled by sass.",
  "type": "module",
  "main": "dist/index.css",
  "module": "dist/index.css",
  "style": "dist/index.css",
  "exports": {
    "./index.css": "./dist/index.css",
    "./dist/index.css": "./dist/index.css",
    "./src/lib/*": "./src/lib/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yisibell/toolkitcss"
  },
  "files": [
    "dist",
    "src/lib"
  ],
  "keywords": [
    "scss",
    "css-toolkit",
    "css",
    "css-class",
    "utility-class"
  ],
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "scripts": {
    "dev": "vite",
    "build:app": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:lib": "rollup -c ./rollup.scss.config.js",
    "release:nobuild": "changelogen --release && npm publish --access=public && git push --follow-tags",
    "release": "npm run build:lib && npm run release:nobuild"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.6",
    "@types/prismjs": "^1.26.3",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "changelogen": "^0.5.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "demoify": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "pinia": "^2.1.7",
    "prettier": "^3.1.1",
    "prismjs": "^1.29.0",
    "rollup": "^4.9.2",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.69.7",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.1",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
  }
}